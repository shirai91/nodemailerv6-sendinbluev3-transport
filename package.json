    "mail",
    "transactional",
    "nodemailer",
    "sendinblue"
  ],
  "author": "Steve Lebleu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shirai91/nodemailerv6-sendinbluev3-transport/issues"
  },
  "homepage": "https://github.com/shirai91/nodemailerv6-sendinbluev3-transport#readme",
  "dependencies": {
    "addressparser": "1.0.1"
  },
  "devDependencies": {
    "auto-changelog": "^2.3.0",
    "coveralls": "3.1.1",
    "mocha": "9.1.3",
    "nodemailer": "6.7.2",
    "nyc": "15.1.0"
  },
  "auto-changelog": {
    "output": "CHANGELOG.md",
